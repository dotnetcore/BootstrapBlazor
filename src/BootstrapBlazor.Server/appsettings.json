{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "LgbFile": {
      "IncludeScopes": true,
      "LogLevel": {
        "Default": "Error"
      },
      "FileName": "Error\\Log.log"
    }
  },
  "AllowedHosts": "*",
  "AllowOrigins": "",
  "ConnectionStrings": {
    "bb": "Data Source=bb.db;"
  },
  "DockViewOptions": {
    "Version": "v1",
    "LocalStorageKeyPrefix": "bb-dockview",
    "EnableLocalStorage": false
  },
  "BootstrapBlazorOptions": {
    "IsPopover": false,
    "ToastDelay": 4000,
    "MessageDelay": 4000,
    "SwalDelay": 4000,
    "EnableErrorLogger": true,
    "FallbackCulture": "en",
    "SupportedCultures": [
      "zh-CN",
      "en-US"
    ],
    "TableSettings": {
      "CheckboxColumnWidth": 36,
      "TableExportOptions": {
        "EnableFormat": true,
        "EnableLookup": true,
        "AutoMergeArray": true,
        "ArrayDelimiter": ","
      }
    },
    "IgnoreLocalizerMissing": true,
    "DisableGetLocalizerFromService": true,
    "DisableGetLocalizerFromResourceManager": true,
    "StepSettings": {
      "Short": "1",
      "Int": "1",
      "Long": "1",
      "Float": "0.1",
      "Double": "0.01",
      "Decimal": "0.01"
    },
    "ConnectionHubOptions": {
      "Enable": true,
      "EnableIpLocator": true,
      "ExpirationScanFrequency": "00:05:00",
      "TimeoutInterval": "00:03:00",
      "BeatInterval": "00:00:30"
    },
    "IpLocatorOptions": {
      "EnableCache": true,
      "SlidingExpiration": "24:00:00",
      "ProviderName": "BaiduIpLocatorProvider"
    },
    "WebClientOptions": {
      "EnableIpLocator": true
    }
  },
  "JuHe": {
    "IpLocatorKey": ""
  },
  "WebsiteOptions": {
    "ServerUrl": "https://www.blazor.zone",
    "TargetFrameworks": ["NET6.0", "NET7.0", "NET8.0", "NET9.0", "NET10.0"],
    "Themes": [
      {
        "key": "bootstrap",
        "name": "Bootstrap",
        "files": []
      },
      {
        "key": "bule",
        "name": "蓝色主题",
        "files": [
          "./css/blue.css"
        ]
      },
      {
        "key": "ant",
        "name": "Ant Design (完善中)",
        "files": [
          "./css/ant.css"
        ]
      },
      {
        "key": "layui",
        "name": "LayUI (完善中)",
        "files": [
          "./css/layui.css"
        ]
      },
      {
        "key": "devui",
        "name": "DevUI",
        "files": [
          "./css/devui.css"
        ]
      }
    ]
  },
  "WebHooks": {
    "Gitee": {
      "Id": "",
      "Token": ""
    }
  },
  "AzureSpeechOption": {
    "SubscriptionKey": "<SubscriptionKey>",
    "Region": "<Region>",
    "AuthorizationTokenUrl": "https://{0}.api.cognitive.microsoft.com/sts/v1.0/issueToken"
  },
  "BaiduSpeechOption": {
    "AppId": "<AppId>",
    "ApiKey": "<ApiKey>",
    "Secret": "<Secret>"
  },
  "BaiduOcrOption": {
    "AppId": "<AppId>",
    "ApiKey": "<ApiKey>",
    "Secret": "<Secret>"
  },
  "AzureOpenAIOption": {
    "Endpoint": "<Endpoint>",
    "Key": "<key>",
    "DeploymentName": "<DeploymentName>"
  },
  "GiteeOptions": {
    "ClientId": "<ClientId>",
    "ClientSecret": "<ClientSecret>",
    "CallbackPath": "/",
    "Scope": "<Scope>"
  },
  "GitHubOptions": {
    "ClientId": "<ClientId>",
    "ClientSecret": "<ClientSecret>",
    "CallbackPath": "/",
    "Scope": "<Scope>"
  },
  "MeiliSearchOptions": {
    "Url": "",
    "ApiKey": "",
    "Index": ""
  }
}

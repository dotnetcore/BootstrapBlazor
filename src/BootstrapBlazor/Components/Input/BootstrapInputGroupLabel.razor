@namespace BootstrapBlazor.Components
@inherits DisplayBase<string>

<DynamicElement @attributes="@AdditionalAttributes" TagName="@TagName" class="@ClassString" style="@StyleString" required="@Required">
    @if (ChildContent != null)
    {
        @ChildContent
    }
    else
    {
        @DisplayText
    }
</DynamicElement>

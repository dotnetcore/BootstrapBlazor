@namespace BootstrapBlazor.Components
@inherits BootstrapComponentBase

<div @attributes="AdditionalAttributes" class="@ClassString" style="@ProgressStyle">
    <div class="@ClassName" role="progressbar" style="@StyleName"
         aria-valuenow="@InternalValue" aria-valuemin="0" aria-valuemax="100">
        @if (ChildContent != null)
        {
            @ChildContent
        }
        else
        {
            @ValueLabelString
        }
    </div>
</div>

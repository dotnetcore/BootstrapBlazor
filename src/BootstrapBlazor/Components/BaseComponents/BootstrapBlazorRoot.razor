@inherits ComponentBase
@namespace BootstrapBlazor.Components

<CascadingValue Value="this" IsFixed="true">
    @if (EnableErrorLoggerValue)
    {
        <ErrorLogger ShowToast="ShowToast" ToastTitle="@ToastTitle" OnErrorHandleAsync="OnErrorHandleAsync!">
            @ChildContent
        </ErrorLogger>
    }
    else
    {
        @ChildContent
    }
</CascadingValue>

<ConnectionHub></ConnectionHub>
<Dialog></Dialog>
<Download></Download>
<DrawerContainer></DrawerContainer>
<Mask></Mask>
<Message @ref="MessageContainer"></Message>
<Print></Print>
<SweetAlert></SweetAlert>
<ToastContainer @ref="ToastContainer"></ToastContainer>

@foreach (var com in Generators)
{
    @com.Generator()
}

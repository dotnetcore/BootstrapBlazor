@using BootstrapBlazor.Components
@using System.Diagnostics.CodeAnalysis
@using ThingsGateway.Razor
@inherits BaseLayout

<Layout SideWidth="0" IsPage="true" IsFullSide="true" IsFixedHeader="true"
        ShowGotoTop="true" ShowCollapseBar="true" ShowTabExtendButtons="false"
        AllowDragTab=true Menus="@MenuService.MenuItems"
        UseTabSet="true" TabDefaultUrl="/">
    <Side></Side>
</Layout>

@code {
    [Inject]
    [NotNull]
    private IMenuService? MenuService { get; set; }
}
